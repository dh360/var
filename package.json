{
	"name": "var",
	"displayName": "var",
	"description": "Help get the proper variable name",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"keybindings": [{
		"command": "px2rem-plus.convert",
		"key": "ctrl+alt+r",
		"when": "editorTextFocus"
	}],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "vaar"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"md5": "^2.2.1",
		"urlencode": "^1.1.0"
	}
}